        <script>
      var em_form = $("#cform");
      function validform() {
        if(em_form.valid()) {
          var url = 'https://www.forwinstock.com.tw/tw/ajax/contact';
          var data = em_form.serialize();
          $.post(url,data,function(a) {
            if(a.msg) {alert(a.msg); return;}
            em_form[0].reset();
            if(a.url) top.location.href=a.url;
          })
        }
      }
      $().ready(function() {
        em_form.validate({
          lang: 'zh-TW',
          rules: {
            xmail: {
              email: true
            }
          }
        })
      });
    </script>
