<script>
  function logout2() {
    $.post("https://www.forwinstock.com.tw/tw/logout/member",function(a) {
      if(a.url) top.location.href = a.url;
    })
  }
</script>
<script>
  function logout() {
    $.post("https://www.forwinstock.com.tw/tw/logout",function(a) {
      if(a.url) top.location.href = a.url;
    })
  }
</script>
