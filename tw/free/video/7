    <script>
    function gotopgae() {
      if(document.referrer) {
          url = document.referrer;
          if(url.indexOf('/teams/teacher')<=-1) top.location.href = 'https://www.forwinstock.com.tw/tw';
          else if(url=='https://www.forwinstock.com.tw/') top.location.href = 'https://www.forwinstock.com.tw/tw';
          else top.location.href = document.referrer;
      } else {
          top.location.href = 'https://www.forwinstock.com.tw/tw';
      }
    }
    </script>
